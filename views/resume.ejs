  <script>
    // Load saved data from localStorage
    const resumeData = JSON.parse(localStorage.getItem('resumeData')) || {};
    let currentTemplate = '';
    
    // Template selection function
    function selectTemplate(templateName) {
      document.getElementById('templateSelection').classList.add('hidden');
      document.getElementById('generatedResume').classList.remove('hidden');
      
      // Hide all templates first
      document.getElementById('professionalTemplate').classList.add('hidden');
      document.getElementById('marketingTemplate').classList.add('hidden');
      document.getElementById('mechanicalTemplate').classList.add('hidden');
      document.getElementById('doctorTemplate').classList.add('hidden');
      document.getElementById('collegeTemplate').classList.add('hidden');
      document.getElementById('copywriterTemplate').classList.add('hidden');
      
      // Show selected template
      currentTemplate = templateName;
      document.getElementById(`${templateName}Template`).classList.remove('hidden');
      
      // Populate template with user data
      populateResume();
    }
    
    // Back to templates
    function backToTemplates() {
      document.getElementById('templateSelection').classList.remove('hidden');
      document.getElementById('generatedResume').classList.add('hidden');
    }
    
    // Format date for display
    function formatDisplayDate(dateString) {
      if (!dateString) return '';
      const date = new Date(dateString);
      return date.toLocaleDateString('en-US', { year: 'numeric', month: 'short' });
    }
    
    // Format date for display (long format)
    function formatLongDisplayDate(dateString) {
      if (!dateString) return '';
      const date = new Date(dateString);
      return date.toLocaleDateString('en-US', { year: 'numeric', month: 'long', day: 'numeric' });
    }
    
    // Calculate skill percentage based on level
    function getSkillPercentage(level) {
      const levels = {
        'beginner': 3,
        'intermediate': 6,
        'advanced': 8,
        'expert': 10
      };
      return levels[level.toLowerCase()] || 5; // Default to intermediate if not matched
    }
    
    // Populate resume with user data
    function populateResume() {
      console.log('Resume Data:', resumeData); // Debug log
      
      // Personal Info
      if (resumeData.personalInfo) {
        const personal = resumeData.personalInfo;
        console.log('Personal Info:', personal); // Debug log
        
        // Name
        if (personal.name) {
          const names = personal.name.split(' ');
          const firstName = names[0] || '';
          const lastName = names.slice(1).join(' ') || '';
          
          if (currentTemplate === 'professional') {
            document.getElementById('resumeName').textContent = 
              `${firstName.toUpperCase()}\n${lastName.toUpperCase()}`;
          } else if (currentTemplate === 'marketing') {
            document.getElementById('marketingResumeName').textContent = 
